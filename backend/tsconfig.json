{
  "compilerOptions": {    
    "target": "ES6",

    /* Modules */
    "module": "CommonJS",                                
    "esModuleInterop": true,                             
    "forceConsistentCasingInFileNames": true,            
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",                                
  },
  "lib": [ 
      "es2015",
    ],    
    "include": [
      "src/**/*.ts"
    ],  
}
